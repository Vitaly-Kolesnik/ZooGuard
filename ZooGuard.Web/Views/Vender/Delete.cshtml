@model VenderViewModel
@{
	ViewData["Title"] = "Delete";
}
<div class="text-center">
    <h3>Delete @Model.Name</h3>
</div>

<div class="form-group">
    <table>
		<tr>
		<th><a class="nav-link text-dark" asp-area="" asp-controller="Vender" asp-action="DeleteVender" asp-route-id="@Model.Id"/> <input type="submit" value="Yes" class="btn btn-primary"/></th> 
		<td><a class="nav-link text-dark" asp-area="" asp-controller="Vender" asp-action="GetAllVendersInDataBase"/><input type="submit" value="No" class="btn btn-primary"/></td>
		</tr>
	</table>
</div>